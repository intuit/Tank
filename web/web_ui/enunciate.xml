<?xml version="1.0" encoding="UTF-8"?>
<enunciate
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.6.0.xsd">

  <title>Intuit Tank  API</title>

  <api-classes>
    <include pattern="com.intuit.tank.api.model.**" />
    <include pattern="com.intuit.tank.api.service.**" />
  </api-classes>

  <modules>
  	<jaxrs>
  		<application path="/rest"/>
  	</jaxrs>
    <c disabled="true" />
    <c-xml-client disable="true"/>
    <obj-c disabled="true" />
    <csharp disabled="true" />
    <csharp-xml-client disabled="true"/>
    <jaxws-client disabled="true" />
    <java-client disabled="false" generateJsonJar="true" />
    
    <docs css="src/main/resources/custom.css" docsSubdir="." includeApplicationPath="true" />
  </modules>
</enunciate> 
